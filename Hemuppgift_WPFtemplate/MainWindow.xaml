<Window x:Class="Hemuppgift_WPFtemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="btnGrid" Content="Grid" Grid.Column="0" Grid.Row="0" FontSize="30" Click="NavigateToGrid"/>
        <Button x:Name="btnStack" Content="StackPanel" Grid.Column="1" Grid.Row="0"  FontSize="30" Click="NavigateToStack"/>
        <Button x:Name="btnWrap" Content="WrapPanel" Grid.Column="0" Grid.Row="1" FontSize="30" Click="NavigateToWrap"/>
        <Button x:Name="btnDock" Content="DockPanel" Grid.Column="1" Grid.Row="1" FontSize="30" Click="NavigateToDock"/>
    </Grid>
</Window>
